{
    "name": "carbon-voyage",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "eslint-config-custom": "workspace:^0.0.0",
        "prettier": "^2.8.3",
        "turbo": "^1.7.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "packageManager": "pnpm@7.25.0"
}
